{
  "expo": {
    "name": "PrefPic",
    "slug": "PrefPic",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.cerenimbus.prefpic"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.cerenimbus.prefpic"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "prefpic",
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "303c7518-5c50-46b3-a077-f3fcfcbc3072"
      }
    }
  }
}
